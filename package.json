{
  "name": "vue-component-seed",
  "version": "1.0.0",
  "description": "Basis Vue reusable-component seed",
  "main": "build/vue-component-seed.js",
  "scripts": {
    "build": "npm run lint && npm run clean:build && npm run webpack && npm run test:unit",
    "clean:build": "rm -rf ./build",
    "clean:modules": "rm -rf ./node_modules",
    "webpack": "webpack --mode production --config ./webpack.config.js",
    "lint": "npm run lint:javascript && npm run lint:vue",
    "lint:javascript": "eslint . --ext js,vue",
    "lint:vue": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "author": "achinarro",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "^6.10.4",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "^4.0.3",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.16",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^4.11.1",
    "webpack-command": "^0.2.0",
    "webpack-merge": "^4.1.0"
  }
}